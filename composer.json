{
    "name": "yujiarong/walmart_api",
    "description": "Provides walmart_api Compatible with HTTP and GuzzleHttp5 in fillup  and GuzzleHttp6,only within the company's internal use ",
    "homepage": "http://guzzlephp.org/",
    "keywords": ["walmart_api"],
    "license": "MIT",
    "authors": [
        {
            "name": "yujiarong",
            "email": "210yujiarong@gmail.com",
            "homepage": "https://github.com/yujiarong"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "react/promise": "^2.2",
        "fillup/walmart-auth-signature-php": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "fillup\\": "src/array2xml/src/",
            "GuzzleHttp\\Command\\Guzzle\\": "src/guzzle-services/src/",
            "GuzzleHttp5\\Command\\": "src/guzzlehttp/command/src/",
            "GuzzleHttp5\\": "src/guzzlehttp/guzzle/src/",
            "GuzzleHttp5\\Subscriber\\Retry\\": "src/guzzlehttp/retry-subscriber/src/",
            "GuzzleHttp5\\Ring\\": "src/guzzlehttp/ringphp/src/",
            "GuzzleHttp5\\Stream\\": "src/guzzlehttp/streams/src/",
            "Walmart\\": "src/walmart-partner-api-sdk-php/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
